# Keycloak Service Environment Configuration
# Copy this file to .env.local and customize for your environment

# Database Configuration
KEYCLOAK_DB_NAME=keycloak_db
KEYCLOAK_DB_USER=keycloak_user
KEYCLOAK_DB_PASSWORD=keycloak_strong_password_123
KEYCLOAK_DB_PORT=5433

# Keycloak Configuration
KEYCLOAK_PORT=8082
KEYCLOAK_HOSTNAME=localhost
KEYCLOAK_ADMIN_USER=admin
KEYCLOAK_ADMIN_PASSWORD=admin_strong_password_123
KEYCLOAK_LOG_LEVEL=INFO

# Redis Configuration (for clustering)
KEYCLOAK_REDIS_PORT=6380
KEYCLOAK_REDIS_PASSWORD=redis_strong_password

# Environment
ENVIRONMENT=development

# URLs
KEYCLOAK_URL=http://localhost:8082
AUTHSERVICE_URL=https://localhost:443
PROJECTSERVICE_URL=http://localhost:5002

# Network Configuration
COMPOSE_PROJECT_NAME=keycloak-service